App.js

#functional component(two types default and named)
const App=()=>{
  let name="chakri";
  return(
    <div>
      <center>
        <h3>Heloo World!! this is {name}</h3>
      </center>
    </div>
  )
}

export default App;


#class based component
import React from "react"

class App extends React.Component{
  render(){
    return(
      <div>
        <center>
          <h3>I am chakri!!</h3>
        </center>
      </div>
    )
  }
}
export default App;


link=https://www.geeksforgeeks.org/differences-between-functional-components-and-class-components/
